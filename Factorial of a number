#include <iostream>
using namespace std;
int main() {
    //Write your code here

        int n;

        long long fact=1;

        int i=1;

        cin>>n;

 

        if(n<0) cout<<"Error";

        else{

            while(i<=n){

                fact = fact*i;

                i++;

            }

            cout<<fact;

        }

 

    return 0;

}
