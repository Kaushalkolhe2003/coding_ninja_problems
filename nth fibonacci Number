#include<bits/stdc++.h>
using namespace std;
int fibbo(int n){
        if(n==0){
                return 0;
        }
        if(n==1 ||n==2){
                return 1;
        }
        else{
                int ans=fibbo(n-2)+fibbo(n-1);
        return ans;
        }
}
int main()
{
       int n;
       cin>>n;
       int c=fibbo(n);
       cout<<c;
}
